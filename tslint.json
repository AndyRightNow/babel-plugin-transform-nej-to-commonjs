{
	"extends": "tslint:recommended",
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true
		],
		"triple-equals": true,
		"quotemark": [
			"single"
		],
		"ordered-imports": false,
		"space-before-function-paren": false,
		"eofline": false,
		"object-literal-sort-keys": [
			false
		]
	}
}